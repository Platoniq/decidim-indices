<%= decidim_form_for(@form, url: admin_update_survey_sections_path, method: :post, html: { class: "form edit_page" }) do |form| %>
  <%= render partial: "form", object: form, locals: { edit_survey_section: t(".edit_survey_section"), add_survey: t(".add_survey") } %>

  <div class="button--double form-general-submit">
    <%= form.submit t(".save") %>
  </div>
<% end %>
